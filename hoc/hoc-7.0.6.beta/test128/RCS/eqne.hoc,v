@### -*-hoc-*-
### ====================================================================
### Test the relational operators == and != with Infinity, NaN, and 
### normal values.
### [19-Dec-2001]
### ====================================================================

### Test (signed) NaN vs (signed) NaN

__TEST__ = "NaN != NaN"
__assert   (NaN != NaN)

__TEST__ = "!(NaN == NaN)"
__assert   (!(NaN == NaN))

__TEST__ = "NaN != -NaN"
__assert   (NaN != -NaN)

__TEST__ = "!(NaN == -NaN)"
__assert   (!(NaN == -NaN))

__TEST__ = "-NaN != NaN"
__assert   (-NaN != NaN)

__TEST__ = "!(-NaN == NaN)"
__assert   (!(-NaN == NaN))

__TEST__ = "-NaN != -NaN"
__assert   (-NaN != -NaN)

__TEST__ = "!(-NaN == -NaN)"
__assert   (!(-NaN == -NaN))

### Test (signed) NaN vs (signed) Inf

__TEST__ = "NaN != Inf"
__assert   (NaN != Inf)

__TEST__ = "!(NaN == Inf)"
__assert   (!(NaN == Inf))

__TEST__ = "NaN != -Inf"
__assert   (NaN != -Inf)

__TEST__ = "!(NaN == -Inf)"
__assert   (!(NaN == -Inf))

__TEST__ = "-NaN != Inf"
__assert   (-NaN != Inf)

__TEST__ = "!(-NaN == Inf)"
__assert   (!(-NaN == Inf))

__TEST__ = "-NaN != -Inf"
__assert   (-NaN != -Inf)

__TEST__ = "!(-NaN == -Inf)"
__assert   (!(-NaN == -Inf))

### Test (signed) NaN vs (signed) finite

__TEST__ = "NaN != MAXNORMAL"
__assert   (NaN != MAXNORMAL)

__TEST__ = "!(NaN == MAXNORMAL)"
__assert   (!(NaN == MAXNORMAL))

__TEST__ = "NaN != -MAXNORMAL"
__assert   (NaN != -MAXNORMAL)

__TEST__ = "!(NaN == -MAXNORMAL)"
__assert   (!(NaN == -MAXNORMAL))

__TEST__ = "-NaN != MAXNORMAL"
__assert   (-NaN != MAXNORMAL)

__TEST__ = "!(-NaN == MAXNORMAL)"
__assert   (!(-NaN == MAXNORMAL))

__TEST__ = "-NaN != -MAXNORMAL"
__assert   (-NaN != -MAXNORMAL)

__TEST__ = "!(-NaN == -MAXNORMAL)"
__assert   (!(-NaN == -MAXNORMAL))

__TEST__ = "NaN != MINNORMAL"
__assert   (NaN != MINNORMAL)

__TEST__ = "!(NaN == MINNORMAL)"
__assert   (!(NaN == MINNORMAL))

__TEST__ = "NaN != -MINNORMAL"
__assert   (NaN != -MINNORMAL)

__TEST__ = "!(NaN == -MINNORMAL)"
__assert   (!(NaN == -MINNORMAL))

__TEST__ = "-NaN != MINNORMAL"
__assert   (-NaN != MINNORMAL)

__TEST__ = "!(-NaN == MINNORMAL)"
__assert   (!(-NaN == MINNORMAL))

__TEST__ = "-NaN != -MINNORMAL"
__assert   (-NaN != -MINNORMAL)

__TEST__ = "!(-NaN == -MINNORMAL)"
__assert   (!(-NaN == -MINNORMAL))

__TEST__ = "NaN != MINSUBNORMAL"
__assert   (NaN != MINSUBNORMAL)

__TEST__ = "!(NaN == MINSUBNORMAL)"
__assert   (!(NaN == MINSUBNORMAL))

__TEST__ = "NaN != -MINSUBNORMAL"
__assert   (NaN != -MINSUBNORMAL)

__TEST__ = "!(NaN == -MINSUBNORMAL)"
__assert   (!(NaN == -MINSUBNORMAL))

__TEST__ = "-NaN != MINSUBNORMAL"
__assert   (-NaN != MINSUBNORMAL)

__TEST__ = "!(-NaN == MINSUBNORMAL)"
__assert   (!(-NaN == MINSUBNORMAL))

__TEST__ = "-NaN != -MINSUBNORMAL"
__assert   (-NaN != -MINSUBNORMAL)

__TEST__ = "!(-NaN == -MINSUBNORMAL)"
__assert   (!(-NaN == -MINSUBNORMAL))

### Test (signed) Inf vs (signed) Inf

__TEST__ = "Inf == Inf"
__assert   (Inf == Inf)

__TEST__ = "Inf != -Inf"
__assert   (Inf != -Inf)

__TEST__ = "-Inf != Inf"
__assert   (-Inf != Inf)

__TEST__ = "-Inf == -Inf"
__assert   (-Inf == -Inf)

### Test (signed) Inf vs (signed) finite

__TEST__ = "Inf != MAXNORMAL"
__assert   (Inf != MAXNORMAL)

__TEST__ = "Inf != -MAXNORMAL"
__assert   (Inf != -MAXNORMAL)

__TEST__ = "-Inf != MAXNORMAL"
__assert   (-Inf != MAXNORMAL)

__TEST__ = "-Inf != -MAXNORMAL"
__assert   (-Inf != -MAXNORMAL)

__TEST__ = "Inf != MINNORMAL"
__assert   (Inf != MINNORMAL)

__TEST__ = "Inf != -MINNORMAL"
__assert   (Inf != -MINNORMAL)

__TEST__ = "-Inf != MINNORMAL"
__assert   (-Inf != MINNORMAL)

__TEST__ = "-Inf != -MINNORMAL"
__assert   (-Inf != -MINNORMAL)

__TEST__ = "Inf != MINSUBNORMAL"
__assert   (Inf != MINSUBNORMAL)

__TEST__ = "Inf != -MINSUBNORMAL"
__assert   (Inf != -MINSUBNORMAL)

__TEST__ = "-Inf != MINSUBNORMAL"
__assert   (-Inf != MINSUBNORMAL)

__TEST__ = "-Inf != -MINSUBNORMAL"
__assert   (-Inf != -MINSUBNORMAL)

### Test (signed) finite vs (signed) finite

__TEST__ = "MAXNORMAL != MINNORMAL"
__assert   (MAXNORMAL != MINNORMAL)

__TEST__ = "MAXNORMAL != -MINNORMAL"
__assert   (MAXNORMAL != -MINNORMAL)

__TEST__ = "-MAXNORMAL != MINNORMAL"
__assert   (-MAXNORMAL != MINNORMAL)

__TEST__ = "-MAXNORMAL != -MINNORMAL"
__assert   (-MAXNORMAL != -MINNORMAL)

__TEST__ = "MAXNORMAL == MAXNORMAL"
__assert   (MAXNORMAL == MAXNORMAL)

__TEST__ = "-MAXNORMAL == -MAXNORMAL"
__assert   (-MAXNORMAL == -MAXNORMAL)

__TEST__ = "MINNORMAL == MINNORMAL"
__assert   (MINNORMAL == MINNORMAL)

__TEST__ = "-MINNORMAL == -MINNORMAL"
__assert   (-MINNORMAL == -MINNORMAL)

__TEST__ = "MINSUBNORMAL == MINSUBNORMAL"
__assert   (MINSUBNORMAL == MINSUBNORMAL)

__TEST__ = "-MINSUBNORMAL == -MINSUBNORMAL"
__assert   (-MINSUBNORMAL == -MINSUBNORMAL)

@
