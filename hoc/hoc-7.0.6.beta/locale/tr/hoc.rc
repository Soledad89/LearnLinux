### Turkish synonyms for some commands: the function name is Turkish,
### but the resulting help will be in English until more work is done.

proc haber()		news()
proc .Mçıkış ()		exit($$1)	## tr

proc yazar() \
{
	_internal_help_hrule()
	print "Hoc seri halinde artan birşekilde farklı uyarlamaları aşağıdaki kitapta\n"
	print "açıklanmaktadır:\n\n"

	print "\tBrian W. Kernighan ve Rob Pike\n"
	print "\tThe UNIX Programming Environment\n"
	print "\tPrentice-Hall\n"
	print "\t1984\n"
	print "\tpp. xii + 357\n"
	print "\tISBN 0-13-937699-2 (hardcover), 0-13-937681-X (paperback)\n"
	print "\tLCCN: QA76.76.O63 K48 1984\n\n"

	print "IEEE 754 aritmatiği, yardım, ek sabitler, ifadeler, ve fonksiyonlar,\n"
	print "başlangıç dosyaları ve GNU destek ekleri\n\n"

	print "\tNelson H. F. Beebe <beebe@math.utah.edu>\n\n"

	print "tarafından ilave edilmiştir.\n\n"

	print "Genişletilmiş hoc'ın resmi Internet arşvine\n\n"

	print "\tftp://ftp.math.utah.edu/pub/hoc\n"
	print "\thttp://www.math.utah.edu/pub/hoc/\n\n"

	print "adresinde bulmak mümkündür.\n"
	_internal_help_hrule()
}

### For now, we provide only a few sample translations.  The welcome
### banner will be printed both in English (from hoc.rc) and in Turkish
### (from this file).

proc _internal_hosgeldiniz() \
{
	_internal_help_hrule()
	print "Yüksek mertebelí genişlet.Aílebílír hesab makínasi,hoc'a,  ho.Mşgeld.Aíníz.\n"
	print "Bu hoc 7.0.0.beta uyarlamasidir (17 Aralik 2001).\n"
	print "Yard.Mım için yardım(), haberler için haber(), yazar hakkında bilgi için\n"
	print "yazar() yazınız.\n"
	if (__IEEE_754__) \
		print "Bu sistem IEEE 754 aritmatik floating-point'ı kullanmaktadır.\n" \
	else \
		print "Bu sistem IEEE 754 aritmatik floating-point'ı DESTEKLEMEMEKTEDİR.\n"
	print "\t****************************************************************\n"
	print "\tNOT: Hoc'taki yardım dosyasının Türkçe'ye tercümesi tamamen\n"
	print "\tbitirimemiştir. Bundan dolayı, kullanıcıların herhangi bir\n"
	print "\tyardımı bizleri mutlu edeceği gibi sizlerin gerek tercümeye\n"
	print "\tgerekse programın gelişmesi için tavsiyeleriniz ayrıyeten\n"
	print "\ttakdir edilecektir. Şimdiden teşekkürler.\n"
	print "\t****************************************************************\n"
	_internal_help_hrule()
}

### Print a welcome banner if option --no-banner was not specified:
if (__BANNER__) _internal_hosgeldiniz()
