	{
		__TEST__ = x "*Yn(" string(k+2) "," x ") == (2*" string(k+1) "*Yn(" string(k+1) "," x ") - " x "*Yn(" k "," x "))"
		__match   (x*Yn(k+2,x), 2*(k+1)*Yn(k+1,x) - x*Yn(k,x))
	}
